{
  "name": "ktourvas/laravel-contacts",
  "type": "simple laravel package managing website contacts",
  "require": {
    "ktourvas/rolesandperms": "^1.3"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "license": "MIT",
  "authors": [
    {
      "name": "Kostas Tourvas",
      "email": "k.tourvas@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "laravel\\contacts\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "laravel\\contacts\\LaravelContactsServiceProvider",
        "laravel\\contacts\\Providers\\EventServiceProvider"
      ]
    }
  }
}
